  },{
    "url": "https://api.storekit-sandbox.itunes.apple.com"
  }],
  "security": [{
    "apple": []
  }],
  "paths": {
    "/v1/apps/{id}/inAppPurchasesV2": {
      "get": {
        "summary": "summary",
        "tags": ["In-App Purchase"],
        "operationId": "getInAppPurchases",
        "parameters": [{
          "in": "path",
          "name": "id",
          "required": true,
          "schema":{
            "type": "string"
          }
        }],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "apple": {
        "type": "oauth2",
        "flows": {
          "x-appleServiceAccount-2": {
            "keyId": "keyId",
            "privateKey": "privateKey",
            "issuerId": "issuerId"
          }
        }
      }
    }
  }
